    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.8",
    "redux-saga": "^0.14.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
