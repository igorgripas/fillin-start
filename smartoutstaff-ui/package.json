    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-addons-perf": "^15.4.2",
    "react-click-outside": "^2.2.0",
    "react-css-modules": "^4.1.0",
    "react-datetime": "^2.8.1",
