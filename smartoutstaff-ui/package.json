{
  "name": "trade-client",
  "version": "2.0.1",
  "description": "Trade client",
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "private": true,
  "scripts": {
    "build": "rm -rf dist && cross-env NODE_ENV='production' webpack --config ./webpack.config.prod.js -p --color --progress && npm run po2json",
    "start": "rm -rf dist/lang npm run po2json && webpack-dev-server  --config ./webpack.config.dev.js --content-base dist --debug --output-pathinfo --watch --inline --port 8095",
    "startLight": "webpack-dev-server  --config ./webpack.config.dev.js --content-base dist --debug --output-pathinfo --watch --inline --port 8095",
    "nodemon": "cross-env BABEL_ENV='node' nodemon server/runner.js",
    "test": "NODE_PATH=./src mocha --compilers js:babel-core/register --require ./test.setup.js './src/**/__tests__/*.@(js|jsx)'",
    "extract_lang": "./bin/extract_lang.sh",
    "po2json": "./bin/po2json.sh"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.22.0",
    "classnames": "^2.2.5",
    "components": "^0.1.0",
